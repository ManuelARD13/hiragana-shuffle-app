{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@common/*": [
        "./common/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@context/*": [
        "./context/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@img/*": [
        "./img/*"
      ],
      "@layout/*": [
        "./layout/*"
      ],
      "@models/*": [
        "./models/*"
      ],
      "@music/*": [
        "./music/*"
      ],
      "@sass/*": [
        "./sass/*"
      ],
      "@services/*": [
        "./services/*"
      ],
    },
    "outDir": "build/dist",
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "src",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest",
    "src/setupTests.ts",
    "./dist/**/*"
  ],
  "types": [
    "typePatches"
  ]
}
